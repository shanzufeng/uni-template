{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
      {
        "path": "pages/tabBar/index",
        "style": {
          "navigationBarTitleText": "首页",
          "navigationStyle": "custom"
        },
        "meta": {
          "auth": false,
          "title": "首页"
        }
      },
      {
        "path": "pages/tabBar/my",
        "style": {
          "navigationBarTitleText": "我的",
          "navigationStyle": "custom"
        },
        "meta": {
          "auth": false,
          "title": "我的"
        }
      }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
  "tabBar": {
    "color": "#666666",
    "borderStyle": "black",
    "selectedColor": "#666666",
    "backgroundColor": "#fff",
    "fontSize": "12px",
    "list": [
      {
        "pagePath": "pages/tabBar/index",
        "iconPath": "static/7.png",
        "selectedIconPath": "static/6.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/tabBar/my",
        "iconPath": "static/9.png",
        "selectedIconPath": "static/8.png",
        "text": "我的"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u-(.*)": "vk-uview-ui/components/u-$1/u-$1.vue",
      "Layout": "@/components/layout/index.vue"
    }
  }
}
